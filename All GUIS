local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("The Darkones Scripts", "DarkTheme")
local Abilities = Window:NewTab("GUIS")
local AbilitiesSection = Abilities:NewSection("GUIS")



AbilitiesSection:NewButton("Prison Life", "", function()

    loadstring(game:HttpGet('https://raw.githubusercontent.com/TheDarkoneMarcillisePex/Other-Scripts/main/Prison%20Life%20GUI'))()
    for i, v in pairs(game.CoreGui:GetDescendants()) do
        if v:IsA("TextLabel") and v.Text == "The Darkones Guis" then
        v.Parent.Parent.Parent:Destroy()
        
        end
        end
end)

AbilitiesSection:NewButton("Swat VS Criminals", "", function()

    loadstring(game:HttpGet('https://raw.githubusercontent.com/TheDarkoneMarcillisePex/Other-Scripts/main/Swat%20Vs%20Crims%20GUI'))()
    for i, v in pairs(game.CoreGui:GetDescendants()) do
        if v:IsA("TextLabel") and v.Text == "The Darkones Guis" then
        v.Parent.Parent.Parent:Destroy()
        
        end
        end
end)

